{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api/index.js"
      }
    ],
    "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
  }
